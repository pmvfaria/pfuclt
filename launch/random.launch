<?xml version="1.0" ?>
<launch>
    <param name="use_sim_time" value="true"/>
    <arg default="/home/$(optenv USER NO_USER_ENV)/datasets/clt_datasets/clt_generated_dataset.bag"
         name="dataset_path"/>
    <arg default="false" name="debug"/>
    <arg default="true" name="publish"/>
    <arg default="1.0" name="rate"/>
    <arg default="250" name="particles"/>
    <arg default="/home/$(optenv USER NO_USER_ENV)/datasets/pfuclt/" name="pfuclt_dataset_path"/>
    <arg default="random_pfuclt.bag" name="pfuclt_dataset_bag"/>
    <param name="num_robots" value="3"/>
    <param name="num_targets" value="2"/>
    <node args="--debug $(arg debug) --publish $(arg publish)" name="pfuclt_node" output="screen" pkg="pfuclt"
          required="true" type="pfuclt_node">
        <param name="percentage_to_keep" value="50"/>
        <param name="velocity_estimator_stack_size" value="10"/>
        <param name="predict_model_stddev" value="30"/>
        <param name="particles" value="$(arg particles)"/>
        <param name="robot_id" value="1"/>
        <param name="K" value="1"/>
        <rosparam param="landmark_covariances">[0.02, 0.005, 0.002, 0.005, 0.005]</rosparam>
        <rosparam param="init_robot_particles">[[[1.2554859383699317, 1.3554859383699318], [3.2160280542623676,
            3.3160280542623672], [2.7631028332870144, 2.863102833287014]], [[3.00560320356958, 3.1056032035695798],
            [-0.7157887222147103, -0.6157887222147103], [-0.29609710524011773, -0.19609710524011775]],
            [[1.9774066525232776, 2.0774066525232775], [4.240041602565133, 4.340041602565132], [1.1841253950143054,
            1.2841253950143054]]]
        </rosparam>
        <rosparam param="init_target_particles">[[[5.575926983620382, 5.675926983620382], [-2.7847601240515005,
            -2.684760124051501], [1.3663356872869128, 1.4663356872869129]], [[5.9470330052086355, 6.047033005208635],
            [-0.03358140825559382, 0.06641859174440619], [0.6937661225432908, 0.7937661225432909]]]
        </rosparam>
    </node>
    <group ns="alphas">
        <rosparam param="robot1">[0.015, 0.1, 0.5, 0.001]</rosparam>
        <rosparam param="robot2">[0.015, 0.1, 0.5, 0.001]</rosparam>
        <rosparam param="robot3">[0.015, 0.1, 0.5, 0.001]</rosparam>
    </group>
</launch>
